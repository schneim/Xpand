{
    "name": "xpand",
    "displayName": "Xpand",
    "publisher": "MarkusSchneider",
    "description": "Xpand Syntax Highlighting Support for VS Code",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/schneim/Xpand.git"
    },
    "contributes": {
        "languages": [{
            "id": "Xpand",
            "aliases": ["Xpand", "Xpand"],
            "extensions": [".xpd",".xpand"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "Xpand",
            "scopeName": "source.Xpand",
            "path": "./syntaxes/Xpand.tmLanguage.json",
            "embeddedLanguages": {
                "source.c": "c"
            }
        }]
    }
}